<div class="card-group">
  <div class="card">
    <img class="card-img" src="{{article.img}}" alt="Card image cap">

  </div>
  <div class="card cardBody">
    <h4 class="card-text">{{article.head}}</h4>
    <p>{{article.par}}</p>
    <h6>by <strong>{{article.author}}</strong></h6>
    <button class="btn" type="button" name="button"><strong>more</strong></button>
  </div>
</div>
